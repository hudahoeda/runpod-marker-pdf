                    "settings": [
                        "pdf", "pdf_base64"
                    ]
                },
                {
                    "title": "Output Settings",
                    "settings": [
                        "output_format", "paginate_output", "disable_image_extraction"
                    ]
                },
                {
                    "title": "Processing Settings",
                    "settings": [
                        "use_llm", "page_range", "force_ocr", "strip_existing_ocr", "languages", "model"
                    ]
                }
